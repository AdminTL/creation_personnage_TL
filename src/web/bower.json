{
  "name" : "Traitre-Lame",
  "dependencies": {
	"angular": "angular#>=1.3.0",
    "angular-bootstrap": "angular-ui-bootstrap-bower#~0.14.3",
	"bootstrap": "bootstrap#~3.0.3",
    "sockjs": "sockjs-client#~0.3.4"
  },
  "install" : {
	"base":  "",
    "path" : "static",
    "sources" : {
      "angular" : "bower_components/angular/*.js",
	  "angular-bootstrap" : "bower_components/angular-bootstrap/*.js",
	  "bootstrap" : [
		  "bower_components/bootstrap/dist/css/*",
		  "bower_components/bootstrap/dist/fonts/*"
	  ],
      "sockjs": "bower_components/sockjs/dist/*.js"
    },
	"ignore" : ["jquery"]
  }
}
